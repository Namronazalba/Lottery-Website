<%#= render "layouts/navbar_admin/navadmin"%>
<div class="main">
  <h1>Edit Category</h1>
  <% flash.each do |type, msg| %>
    <div class="alert alert-info">
      <%= msg %>
    </div>
  <% end %>
  <%= form_for @category, url: admin_category_path, method: :patch do |f| %>
    <%= render partial: 'form', locals: {f: f, category: @category} %> <br>
    <%= f.submit "Update",class: 'btn btn-primary ' %>
  <% end %>
</div>
