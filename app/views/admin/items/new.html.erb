<%= render "layouts/navbar_admin/navadmin"%>
<div class="main">
  <h1>Add new Item</h1>
  <%= render "layouts/errormsg/errormsg" %>
  <%= form_for @item, url: admin_items_path, method: :post do |f| %>
    <%= render partial: 'form', locals: {f: f, item: @item} %> <br>
      <%= f.submit "Save",class: 'btn btn-primary', style:'margin-buttom: 10px;' %><br><br>
  <% end %>
</div>