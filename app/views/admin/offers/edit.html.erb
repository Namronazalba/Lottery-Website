<%= render "layouts/navbar_admin/navadmin"%>
<div class="main">
  <h1>Edit Item</h1>
  <%= render "layouts/errormsg/errormsg" %>
  <%= form_for @offer, url: admin_offer_path, method: :patch do |f| %>
