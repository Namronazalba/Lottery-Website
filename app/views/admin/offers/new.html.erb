<%= render "layouts/navbar_admin/navadmin"%>
<div class="main">
  <h1>Add new Offer</h1>
  <%= form_for @offer, url: admin_offers_path, method: :post do |f| %>
    <%= render partial: 'form', locals: {f: f, offer: @offer} %> <br>
    <%= f.submit "Save",class: 'btn btn-primary', style:'margin-buttom: 10px;' %><br><br>
  <% end %>
</div>